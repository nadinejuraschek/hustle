rt styles from './details.module.css';\n\n// HOOKS\nimport { useWindowSize } from 'hooks/useWindowSize';\n\nconst Details = () => {\n  const size = useWindowSize();\n  const [ list, setList ] = useState([]);\n\n  return (\n    <>\n    {\n      size.width < 1024 ?\n      <>\n      <Graph list={list} />\n      <div className={styles.listContainer}>\n        <Filter handleList={setList} />\n        <List list={list} />\n      </div>\n      </>\n      :\n      size.width < 1280 ?\n      <div className={styles.desktopGrid}>\n        <Graph list={list} />\n        <div className={styles.listContainer}>\n          <Filter handleList={setList} />\n          <List list={list} />\n        </div>\n      </div>\n      :\n      <div className={styles.largeGrid}>\n        <Graph list={list} />\n        <div className={styles.listContainer}>\n          <Filter handleList={setList} />\n          <List list={list} />\n        </div>\n        <AddBar />\n      </div>\n    }\n    </>\n  );\n};\n\nexport default Details;","/Users/mymac/Documents/WebDev/Projects/hustle/src/components/List/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Graph/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Icon/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Modal/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Color/index.jsx",[],["131","132"],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Spinner/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/reducers/AppReducer.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/context/GlobalContext.jsx",["133"],"// REACT\nimport { createContext, useEffect, useReducer, useState } from 'react';\n\n// REDUCERS\nimport { AppReducer } from 'reducers/AppReducer';\n\nconst initialState = {\n  jobs: [\n    { label: \"Babysitten\", value: 'babysitten', income: 0 },\n    { label: \"Nachhilfe\", value: 'nachhilfe', income: 0  },\n    { label: \"Kleiderverkauf\", value: 'kleiderverkauf', income: 0 },\n    { label: \"Tanzstunden\", value: 'tanzstunden', income: 0 },\n    // { label: \"Minijob\", value: 'minijob' },\n  ],\n  transactions: [\n    { job: 'babysitten', source: 'Familie Müller', amount: 30, timestamp: 1606999499 },\n    { job: 'babysitten', source: 'Familie Bauer', amount: 50, timestamp: 1606826699 },\n    { job: 'kleiderverkauf', source: 'Vinted', amount: 22, timestamp: 1606826699 },\n    { job: 'kleiderverkauf', source: 'eBay', amount: 8, timestamp: 1605876299 },\n    { job: 'tanzstunden', source: 'TV Baienfurt', amount: 64, timestamp: 1606221899 },\n    // { job: 'minijob', source: 'GEW', amount: 120, timestamp: 1606394699 },\n    { job: 'nachhilfe', source: 'Familie Berg', amount: 10, timestamp: 1606481099 },\n  ],\n};\n\nexport const GlobalContext = createContext(initialState);\n\nexport const GlobalContextProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(AppReducer, initialState);\n  const [incomeList, setIncomeList] = useState([]);\n\n  const addJob = job => {\n    dispatch({\n      type: \"ADD_JOB\",\n      payload: job\n    });\n  };\n\n  const addTransaction = transaction => {\n    dispatch({\n      type: \"ADD_TRANSACTION\",\n      payload: transaction\n    });\n  };\n\n  // DISPLAY TOTAL INCOMES\n  useEffect(() => {\n    const jobs = state.jobs;\n    const transactions = state.transactions;\n\n    let graphIncomes = [];\n    // loop over jobs\n    jobs.map(job => {\n      const name = job.value;\n      let total = 0;\n      // loop over transactions\n      // find all matching transactions\n      const sortedTransactions = transactions.filter(transaction => transaction.job === name);\n      // loop over matching transactions to add incomes\n      sortedTransactions.map(item => {\n        total = total + item.amount\n      });\n      // create array entry\n      // push to array\n      return graphIncomes.push(total);\n    });\n    setIncomeList(graphIncomes);\n  }, [state]);\n\n  return (\n    <GlobalContext.Provider value={{\n      jobs: state.jobs,\n      transactions: state.transactions.sort((x, y) => {\n        return x.timestamp - y.timestamp;\n      }),\n      incomeList,\n      addTransaction,\n      addJob,\n    }}>\n      { children }\n    </GlobalContext.Provider>\n  );\n};","/Users/mymac/Documents/WebDev/Projects/hustle/src/components/LanguageButtons/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Sidebar/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/hooks/useWindowSize.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Filter/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/AddBar/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/helpers/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Tabs/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Filter/Dropdown/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Filter/Dropdown/AddJobForm/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/List/ListItem/index.jsx",[],{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","severity":1,"message":"139","line":2,"column":10,"nodeType":"140","messageId":"141","endLine":2,"endColumn":20},{"ruleId":"138","severity":1,"message":"142","line":2,"column":22,"nodeType":"140","messageId":"141","endLine":2,"endColumn":31},{"ruleId":"134","replacedBy":"143"},{"ruleId":"136","replacedBy":"144"},{"ruleId":"145","severity":1,"message":"146","line":60,"column":35,"nodeType":"147","messageId":"148","endLine":60,"endColumn":37},"no-native-reassign",["149"],"no-negated-in-lhs",["150"],"no-unused-vars","'useContext' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used.",["149"],["150"],"array-callback-return","Array.prototype.map() expects a return