sx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Layout/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Navbar/index.jsx",["118"],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Footer/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/pages/Dashboard/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/pages/Create/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Form/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/i18n.js",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Input/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Button/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Details/index.jsx",["119","120"],"// REACT\nimport { useContext, useEffect, useState } from 'react';\n\n// DEPENDENCIES\n\n// COMPONENTS\nimport AddBar from 'components/AddBar';\nimport Graph from 'components/Graph';\nimport Filter from 'components/Filter';\nimport List from 'components/List';\n\n// STYLES\nimport styles from './details.module.css';\n\n// HOOKS\nimport { useWindowSize } from 'hooks/useWindowSize';\n\nconst Details = () => {\n  const size = useWindowSize();\n  const [ list, setList ] = useState([]);\n\n  return (\n    <>\n    {\n      size.width < 1024 ?\n      <>\n      <Graph list={list} />\n      <div className={styles.listContainer}>\n        <Filter handleList={setList} />\n        <List list={list} />\n      </div>\n      </>\n      :\n      size.width < 1280 ?\n      <div className={styles.desktopGrid}>\n        <Graph list={list} />\n        <div className={styles.listContainer}>\n          <Filter handleList={setList} />\n          <List list={list} />\n        </div>\n      </div>\n      :\n      <div className={styles.largeGrid}>\n        <Graph list={list} />\n        <div className={styles.listContainer}>\n          <Filter handleList={setList} />\n          <List list={list} />\n        </div>\n        <AddBar />\n      </div>\n    }\n    </>\n  );\n};\n\nexport default Details;","/Users/mymac/Documents/WebDev/Projects/hustle/src/components/List/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Graph/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Icon/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Modal/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Color/index.jsx",[],["121","122"],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Spinner/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/reducers/AppReducer.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/context/GlobalContext.jsx",["123"],"// REACT\nimport { createContext, useEffect, useReducer, useState } from 'react';\n\n// REDUCERS\nimport { AppReducer } from 'reducers/AppReducer';\n\nconst initialState = {\n  jobs: [\n    { label: \"Babysitten\", value: 'babysitten', income: 0 },\n    { label: \"Nachhilfe\", value: 'nachhilfe', income: 0  },\n    { label: \"Kleiderverkauf\", value: 'kleiderverkauf', income: 0 },\n    { label: \"Tanzstunden\", value: 'tanzstunden', income: 0 },\n    // { label: \"Minijob\", value: 'minijob' },\n  ],\n  transactions: [\n    { job: 'babysitten', source: 'Familie MÃ¼ller', amount: 30, timestamp: 1606999499 },\n    { job: 'babysitten', source: 'Familie Bauer', amount: 50, timestamp: 1606826699 },\n    { job: 'kleiderverkauf', source: 'Vinted', amount: 22, timestamp: 1606826699 },\n    { job: 'kleiderverkauf', source: 'eBay', amount: 8, timestamp: 1605876299 },\n    { job: 'tanzstunden', source: 'TV Baienfurt', amount: 64, timestamp: 1606221899 },\n    // { job: 'minijob', source: 'GEW', amount: 120, timestamp: 1606394699 },\n    { job: 'nachhilfe', source: 'Familie Berg', amount: 10, timestamp: 1606481099 },\n  ],\n};\n\nexport const GlobalContext = createContext(initialState);\n\nexport const GlobalContextProvider = ({ children }) => {\n  const [state, dispatch] = useReducer(AppReducer, initialState);\n  const [incomeList, setIncomeList] = useState([]);\n\n  const addJob = job => {\n    dispatch({\n      type: \"ADD_JOB\",\n      payload: job\n    });\n  };\n\n  const addTransaction = transaction => {\n    dispatch({\n      type: \"ADD_TRANSACTION\",\n      payload: transaction\n    });\n  };\n\n  // DISPLAY TOTAL INCOMES\n  useEffect(() => {\n    const jobs = state.jobs;\n    const transactions = state.transactions;\n\n    let graphIncomes = [];\n    // loop over jobs\n    jobs.map(job => {\n      const name = job.value;\n      let total = 0;\n      // loop over transactions\n      // find all matching transactions\n      const sortedTransactions = transactions.filter(transaction => transaction.job === name);\n      // loop over matching transactions to add incomes\n      sortedTransactions.map(item => {\n        total = total + item.amount\n      });\n      // create array entry\n      // push to array\n      return graphIncomes.push(total);\n    });\n    setIncomeList(graphIncomes);\n  }, [state]);\n\n  return (\n    <GlobalContext.Provider value={{\n      jobs: state.jobs,\n      transactions: state.transactions.sort((x, y) => {\n        return x.timestamp - y.timestamp;\n      }),\n      incomeList,\n      addTransaction,\n      addJob,\n    }}>\n      { children }\n    </GlobalContext.Provider>\n  );\n};","/Users/mymac/Documents/WebDev/Projects/hustle/src/components/LanguageButtons/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Sidebar/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/hooks/useWindowSize.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Filter/index.jsx",["124"],"// REACT\nimport { useContext, useEffect, useState } from 'react';\n\n// TRANSLATION\nimport { useTranslation } from 'react-i18next';\n\n// STYLES\nimport styles from './filter.module.css';\n\n// CONTEXT\nimport { GlobalContext } from 'context/GlobalContext';\n\n// ICONS\nimport { Plus } from 'components/Icon';\nimport { Save } from 'components/Icon';\nimport { ReactComponent as ChevronDown } from 'assets/icons/chevron-down.svg';\n\nconst Filter = ({ handleList }) => {\n  const { t } = useTranslation();\n  const { jobs, transactions, addJob } = useContext(GlobalContext);\n\n  // Dropdown logic\n  const [ dropdown, setDropdown ] = useState(false);\n  const [ selectedJob, setSelectedJob ] = useState({ label: t('FORM.JOB.ALL'), value: '', income: 0 });\n\n  const handleDropdown = () => {\n    if (dropdown === false) {\n      setDropdown(true);\n    } else {\n      setJobForm(false);\n      setDropdown(false);\n    };\n  };\n\n  const handleSelectedJob = (label) => {\n    setSelectedJob(label);\n    setDropdown(false);\n  };\n\n  // ADD JOB logic\n  const [ inputValue, setInputValue ] = useState('');\n  const [ jobForm, setJobForm ] = useState(false);\n\n  const handleInputChange = event => {\n    const value = event.target.value;\n    setInputValue(value);\n  };\n\n  const handleJobSave = event => {\n    event.preventDefault();\n    const data = { label: inputValue, value: inputValue.toLowerCase(), income: 0 };\n    addJob(data);\n    setJobForm(false);\n  };\n\n  // FILTER TRANSACTIONS logic\n  useEffect(() => {\n    if (selectedJob.value === '') {\n      handleList(transactions);\n    } else {\n      const filteredList = transactions.filter(transaction => transaction.job === selectedJob.value);\n      handleList(filteredList);\n    };\n  }, [transactions, selectedJob]);\n\n  return (\n    <div className={styles.filter}>\n      <div className={styles.filterBtn} onClick={() => handleDropdown()}>\n        {selectedJob.label}\n        <ChevronDown className={styles.down} />\n      </div>\n      {dropdown && (\n        <div className={styles.dropdown}>\n          <div\n            className={styles.item}\n            onClick={() => handleSelectedJob({ label: t('FORM.JOB.ALL'), value: '' })}\n          >\n            {t('FORM.JOB.ALL')}\n          </div>\n          {jobs.length > 1 &&\n            jobs.map((job, index) => {\n              return (\n                <div\n                  className={styles.item}\n                  key={index}\n                  onClick={() => handleSelectedJob(job)}\n                >\n                  {job.label}\n                </div>\n              );\n            })}\n          {jobForm ? (\n            <div className={styles.formItem}>\n              <input\n                className={styles.input}\n                placeholder={t('FORM.JOB.NEW')}\n                onChange={handleInputChange}\n                value={inputValue.label}\n              />\n              <Save className={styles.icon} onClick={handleJobSave} />\n            </div>\n          ) : (\n            <div className={styles.addItem} onClick={() => setJobForm(true)}>\n              <Plus className={styles.icon} />\n              <span>{t('FORM.JOB.ADD')}</span>\n            </div>\n          )}\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default Filter;\n","/Users/mymac/Documents/WebDev/Projects/hustle/src/components/AddBar/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/helpers/index.jsx",[],"/Users/mymac/Documents/WebDev/Projects/hustle/src/components/Tabs/index.jsx",[],{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","severity":1,"message":"130","line":6,"column":10,"nodeType":"131","messageId":"132","endLine":6,"endColumn":16},{"ruleId":"129","severity":1,"message":"133","line":2,"column":10,"nodeType":"131","messageId":"132","endLine":2,"endColumn":20},{"ruleId":"129","severity":1,"message":"134","line":2,"column":22,"nodeType":"131","messageId":"132","endLine":2,"endColumn":31},{"ruleId":"125","replacedBy":"135"},{"ruleId":"127","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":60,"column":35,"nodeType":"139","messageId":"140","end